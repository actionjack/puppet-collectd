<LoadPlugin python>
    Globals true
</LoadPlugin>

<Plugin python>
    # mongodb.py is at path /opt/collectd-plugins/mongodb.py
    ModulePath "/opt/collectd-plugins/collectd-mongodb"

    Import "mongodb"
    <Module mongodb>
        Host     "<%= fqdn %>"
        Database "smartSteps"
    </Module>
</Plugin>
